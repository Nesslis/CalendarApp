{
  "expo": {
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      "expo-localization",
      "expo-secure-store"
    ],
    "name": "Calendar App",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/brand.png",
    "splash": {
      "image": "./app/assets/images/brand.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.nessli.myapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/brand.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nessli.myapp"
    },
    "web": {
      "favicon": "./app/assets/core/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "485aa073-758c-4889-b485-da4e037d4708"
      }
    }
  }
}
